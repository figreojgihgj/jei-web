                "text": {
                  "inlineElements": [
                    {
                      "bold": true,
                      "color": "light_text_primary",
                      "kind": "text",
                      "text": {
                        "text": "击败碾骨焰术师的掉落物，可用于精制食药制作。"
                      }
                    }
                  ],
                  "kind": "body"
                }
              }
